            <discovery_rules>
                <discovery_rule>
                    <name>IBM Storwize Discovery Battery</name>
                    <key>storwize.discovery[{$STORWIZEADDR},battery]</key>
                    <delay>1h</delay>
                    <lifetime>0</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Status Battery {#VOLNAME}</name>
                            <key>storwize.battery.stats[{#STORWIZEADDR},{#VOLNAME},status]</key>
                            <delay>1h</delay>
                            <applications>
                                <application>
                                    <name>Status Battery</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>Storage IBM</name>
                            </valuemap>
                            <preprocessing>
                                <step>
                                    <type>JAVASCRIPT</type>
                                    <parameters>
                                        <parameter>var stats = 3;

if(value.includes('offline')){
  stats = 0;
}

if(value.includes('online')){
  stats = 1;
}

return stats;</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&lt;&gt;1</expression>
                                    <name>Battery {#VOLNAME} is {ITEM.LASTVALUE}</name>
                                    <priority>DISASTER</priority>
                                    <manual_close>YES</manual_close>
                                    <tags>
                                        <tag>
                                            <tag>Prumo - Storage</tag>
                                            <value>Battery</value>
                                        </tag>
                                    </tags>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
